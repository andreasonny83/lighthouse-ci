{
  "name": "lighthouse-ci",
  "version": "1.9.0",
  "description": "CLI implementation for running Lighthouse with any CI tool",
  "scripts": {
    "test": "npm run format && xo && jest",
    "test:watch": "jest --watchAll",
    "format": "prettier --write \"lib/**/*.{js,js}\"",
    "contributors:add": "all-contributors add",
    "contributors:generate": "all-contributors generate",
    "release": "np"
  },
  "dependencies": {
    "chrome-launcher": "^0.11.1",
    "lighthouse": "^5.1.0",
    "meow": "^5.0.0",
    "mkdirp": "^0.5.1",
    "ora": "^3.4.0",
    "rimraf": "^2.6.3",
    "update-notifier": "^3.0.1"
  },
  "devDependencies": {
    "@types/jest": "^24.0.15",
    "@types/ora": "^3.2.0",
    "all-contributors-cli": "^6.8.1",
    "chalk": "^2.4.2",
    "jest": "^24.8.0",
    "np": "^5.0.3",
    "prettier": "^1.18.2",
    "xo": "^0.24.0"
  },
  "keywords": [
    "devtools",
    "lighthouse",
    "ci"
  ],
  "bin": {
    "lighthouse-ci": "bin/cli"
  },
  "files": [
    "lib",
    "bin",
    "README.md",
    "LICENSE"
  ],
  "engines": {
    "node": ">=6"
  },
  "xo": {
    "prettier": true,
    "envs": [
      "node",
      "es6",
      "jest"
    ],
    "rules": {
      "max-params": ["error", 5]
    }
  },
  "author": "Andrea Sonny <andreasonny83@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/andreasonny83/lighthouse-ci.git"
  },
  "bugs": {
    "url": "https://github.com/andreasonny83/lighthouse-ci.git/issues"
  },
  "homepage": "https://github.com/andreasonny83/lighthouse-ci.git#readme"
}
